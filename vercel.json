{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/ai/(.*)",
      "dest": "http://172.25.234.163:8081/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_AI_API_URL": "http://172.25.234.163:8081",
    "NEXT_PUBLIC_AI_DOMAIN": "ai.mountainshares.us"
  }
}
