{
  "version": 2,
  "routes": [
    {
      "src": "/api/ai/(.*)",
      "dest": "http://YOUR_PUBLIC_IP:8081/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_AI_API_URL": "http://YOUR_PUBLIC_IP:8081",
    "NEXT_PUBLIC_AI_DOMAIN": "ai.mountainshares.us"
  }
}
